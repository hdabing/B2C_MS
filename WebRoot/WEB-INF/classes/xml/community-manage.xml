<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 2.1//EN" "http://struts.apache.org/dtds/struts-2.1.dtd">
<struts>
	<package name="community-manage" extends="com.yizhaobao" namespace="/community">
		<action name="" class="CommunityAction" method="doNothing">
			<result>/community_manage/community.jsp</result>
		</action>
		<action name="add" class="CommunityAction" method="enterAddCommunity">
			<result name="success">/community_manage/create_community.jsp</result>
		</action>
		<action name="save" class="CommunityAction" method="save">
			<result type="redirect">/community</result>
		</action>
	</package>
</struts>
